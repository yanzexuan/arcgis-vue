{
  "name": "arcgis-vue",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "element-ui": "^2.12.0",
    "esri-loader": "^2.10.1",
    "vue": "^2.6.10",
    "vue-router": "^3.1.3",
    "vuex": "^3.1.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.11.0",
    "@vue/cli-plugin-eslint": "^3.11.0",
    "@vue/cli-service": "^3.11.0",
    "eslint-plugin-html": "^6.0.0",
    "eslint-plugin-vuefix": "^0.2.1",
    "js-yaml": "^3.13.1",
    "less": "^3.10.3",
    "less-loader": "^4.1.0",
    "lint-staged": "^7.2.2",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-var": "error",
      "indent": [
        "error",
        2
      ],
      "semi": [
        "error",
        "always"
      ],
      "semi-style": [
        "error",
        "last"
      ],
      "semi-spacing": [
        "error",
        {
          "before": false,
          "after": true
        }
      ],
      "no-dupe-keys": "error",
      "no-eval": "error",
      "no-alert": "error",
      "no-eq-null": "error",
      "no-console": "off",
      "max-len": [
        "error",
        {
          "code": 120
        }
      ],
      "max-lines": [
        "error",
        {
          "max": 200,
          "skipBlankLines": true,
          "skipComments": true
        }
      ]
    },
    "plugins": [
      "vuefix",
      "html"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
